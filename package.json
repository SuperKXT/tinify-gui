{
	"name": "tinify-gui",
	"productName": "tinify-gui",
	"version": "0.0.1",
	"description": "GUI for Tinify API written in Typescript, React, and Electron",
	"main": ".webpack/main",
	"scripts": {
		"start": "tsc && electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint .",
		"svgr": "yarn svgr:remove-icons && yarn svgr:generate-icons && yarn svgr:rename-index",
		"svgr:generate-icons": "svgr --config-file ./.svgr/svgr.config.js ./src/media/icons/",
		"svgr:remove-icons": "rimraf ./src/components/branding/icons",
		"svgr:rename-index": "find ./src/components/branding/icons | renamer --find index.icon.tsx --replace index.tsx"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": { },
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "tinify_gui"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": { }
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": { }
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/index.html",
									"js": "./src/renderer.tsx",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"@electron-forge/cli": "^6.0.0-beta.66",
		"@electron-forge/maker-deb": "^6.0.0-beta.66",
		"@electron-forge/maker-rpm": "^6.0.0-beta.66",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.66",
		"@electron-forge/maker-zip": "^6.0.0-beta.66",
		"@electron-forge/plugin-webpack": "^6.0.0-beta.66",
		"@svgr/cli": "^6.3.1",
		"@svgr/core": "^6.3.1",
		"@svgr/webpack": "^6.3.1",
		"@types/eslint": "^8.4.6",
		"@types/jest": "^29.0.3",
		"@types/node": "^18.7.18",
		"@types/react": "^18.0.20",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@vercel/webpack-asset-relocator-loader": "^1.7.3",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"electron": "20.1.4",
		"eslint": "^8.23.1",
		"eslint-plugin-import": "^2.25.0",
		"eslint-plugin-react": "^7.31.8",
		"fork-ts-checker-webpack-plugin": "^7.2.13",
		"jest": "^29.0.3",
		"msw": "^0.47.3",
		"node-loader": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"renamer": "^4.0.0",
		"rimraf": "^3.0.2",
		"style-loader": "^3.3.1",
		"svg-url-loader": "^8.0.0",
		"ts-loader": "^9.3.1",
		"typescript": "~4.5.4"
	},
	"dependencies": {
		"electron-squirrel-startup": "^1.0.0"
	}
}
